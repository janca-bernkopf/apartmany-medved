<section class="services">
    {% for items in services %}
    <div class="services__set">
        <img class="services__icon" src="/images/{{items.icon}}" alt="Čas příjezdu a odjezdu">
        <h4 class="services__title">{{items.title | safe}}</h4>
        <p class="services__txt">{{items.description | safe}}</p>
    </div>
    {% endfor %}
</section>